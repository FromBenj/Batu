{% extends 'layout/base.html.twig' %}
{% block content %}
<div class="page-title text-center fw-bold">{{ "app.professional.service.new"|trans }}</div>
    {% form_theme form_service 'bootstrap_5_layout.html.twig' %}
    <form method="post" action="{{ path('professional_new-service') }}" autocomplete="off">
        {{ form_row(form_service.category) }}
        {{ form_row(form_service.languages) }}
        {{ form_row(form_service.specialization) }}
        {{ form_row(form_service.description) }}
        <div id="address-container">
            <label class="form-label required">Address</label>
            <input type="text" id="service_address" maxlength="255" class="form-control">
        </div>
        {{ form_row(form_service.housenumber) }}
        {{ form_row(form_service.street) }}
        {{ form_row(form_service.city) }}
        {{ form_row(form_service.postcode) }}
        {{ form_row(form_service.country) }}
        {{ form_row(form_service.county) }}
        {{ form_row(form_service.latitude) }}
        {{ form_row(form_service.longitude) }}
        {{ form_row(form_service.addressDetails) }}
        {{ form_row(form_service.price) }}
        <input type="submit" class="btn accordion-button" value="go">
    {{ form_end(form_service) }}
{% endblock %}
